{
  "name": "@confytome/confluence",
  "version": "1.7.11",
  "description": "Cross-platform Markdown to Confluence converter - converts .md files to Confluence-compatible HTML format and copies to clipboard for direct pasting.",
  "main": "standalone-generator.js",
  "bin": {
    "confytome-confluence": "./cli.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n-ae/confytome.git",
    "directory": "packages/confluence"
  },
  "bugs": {
    "url": "https://github.com/n-ae/confytome/issues"
  },
  "homepage": "https://github.com/n-ae/confytome/tree/main/packages/confluence#readme",
  "keywords": [
    "confytome-plugin",
    "confluence",
    "wiki-markup",
    "confluence-cloud",
    "confluence-server",
    "atlassian",
    "mustache",
    "openapi",
    "api-documentation",
    "generator",
    "confytome"
  ],
  "author": "nae",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "standalone-generator.js",
    "cli.js",
    "utils/",
    "templates/",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Tests in main confytome package\" && exit 0",
    "standalone": "node cli.js",
    "lint": "eslint . --ext .js",
    "format": "eslint . --ext .js --fix",
    "clean": "rm -rf docs/* .cache",
    "validate": "npm ls",
    "prepublishOnly": "npm run validate"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "clipboardy": "^4.0.0",
    "marked": "^12.0.0",
    "@confytome/markdown": "^1.7.6"
  },
  "peerDependencies": {
    "@confytome/core": "^1.4.0"
  },
  "peerDependenciesMeta": {
    "@confytome/core": {
      "optional": true
    }
  }
}
