  "main": "./generate-swagger.js",
  "className": "SwaggerUIGenerator",
  "version": "1.4.0",
  "author": "nae",
  "tags": [
    "swagger",
    "interactive",
    "swagger-ui"
  ],
  "dependencies": {
    "commander": "^14.0.0",
