{
  "name": "@confytome/generator",
  "version": "2.0.1",
  "description": "Template generation system for confytome CLI files, manifests, and documentation",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "confytome-generator": "./scripts/cli.js"
  },
  "scripts": {
    "clean": "rm -rf temp/* .cache",
    "regenerate:cli": "node scripts/regenerate-cli-files.js",
    "regenerate:readmes": "node scripts/regenerate-readmes.js",
    "regenerate:readmes:quality": "node scripts/validate-readme-quality.js",
    "test": "echo \"No tests for generator package\" && exit 0"
  },
  "keywords": [
    "confytome",
    "template",
    "generator",
    "cli",
    "mustache",
    "documentation"
  ],
  "author": "nae",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n-ae/confytome.git",
    "directory": "packages/generator"
  }
}
