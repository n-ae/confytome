{
  "name": "@confytome/core",
  "version": "1.8.4",
  "description": "Core plugin system and OpenAPI 3.0.3 generator from JSDoc comments. Plugin registry, service layer, and CLI for extensible API documentation generation.",
  "main": "generate-openapi.js",
  "exports": {
    ".": "./generate-openapi.js",
    "./constants.js": "./constants.js",
    "./utils/*": "./utils/*",
    "./services/*": "./services/*",
    "./interfaces/*": "./interfaces/*"
  },
  "bin": {
    "confytome": "cli.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n-ae/confytome.git"
  },
  "bugs": {
    "url": "https://github.com/n-ae/confytome/issues"
  },
  "homepage": "https://github.com/n-ae/confytome#readme",
  "keywords": [
    "plugin-system",
    "generator-registry",
    "openapi",
    "jsdoc",
    "api-documentation",
    "swagger",
    "cli",
    "service-layer",
    "dependency-injection",
    "extensible",
    "core",
    "confytome"
  ],
  "author": "nae",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "cli.js",
    "generate-openapi.js",
    "utils/",
    "services/",
    "interfaces/",
    "templates/",
    "constants.js",
    "README.md"
  ],
  "scripts": {
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf docs/* .cache",
    "dev": "npm run clean && npm run validate",
    "validate": "npm run lint && npm test",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "prepublishOnly": "npm run validate"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "glob": "^11.0.0",
    "swagger-jsdoc": "^6.2.8"
  }
}
